[Transfer]
ProtectVersion=%A

[Source]
Type=url-file
Path=http://mangos.soren.tools/sysupdate/mangos
MatchPattern=%M_@v.root-%a.@u.raw \
             %M_@v.root-%a.@u.raw.gz \
             %M_@v.root-%a.@u.raw.zst \
             %M_@v.root-%a.@u.raw.xz

[Target]
Type=partition
Path=auto
MatchPattern=%M_@v
MatchPartitionType=root
PartitionFlags=0
ReadOnly=1
