[Transfer]
ProtectVersion=%A
Features=debug

[Source]
Type=url-file
Path=http://localhost:1002/
MatchPattern=debug_@v.raw \
             debug_@v.raw.gz \
             debug_@v.raw.zst \
             debug_@v.raw.xz \

[Target]
Type=regular-file
Path=/var/lib/extensions/debug.raw.v/
InstancesMax=2
MatchPattern=debug_@v.raw
Mode=0444
