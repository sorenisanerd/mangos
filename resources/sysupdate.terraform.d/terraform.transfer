[Source]
Type=url-file
Path=http://mangos.soren.tools/sysupdate/terraform
MatchPattern=terraform_@v.raw \
             terraform_@v.raw.gz \
             terraform_@v.raw.zst \
             terraform_@v.raw.xz \

[Target]
Type=regular-file
Path=/var/lib/extensions/terraform.raw.v/
InstancesMax=2
MatchPattern=terraform_@v.raw
Mode=0444
